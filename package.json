{
  "name": "movie",
  "version": "1.0.0",
  "description": "Movies",
  "scripts": {
    "client": "cd ./client && npm run dev",
    "server": "cd ./server && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "cd ./client && npm install",
    "server-install": "cd ./server && npm install",
    "dev-install": "npm run server-install && npm run client-install",
    "prepare": "husky install",
    "client-lint": "cd ./client && npm run lint"
  },
  "author": "NA",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.2",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.10",
    "movie-trailer": "^2.1.0",
    "react": "^17.0.2",
    "react-hook-form": "^7.27.0",
    "react-router-dom": "^5.2.0",
    "react-youtube": "^7.14.0",
    "request": "^2.88.2",
    "ts-node": "^10.4.0",
    "youtube": "^0.1.0"
  },
  "devDependencies": {
    "@types/react-autosuggest": "^10.1.5",
    "concurrently": "^5.3.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.6"
  }
}
