{
    "env": {
        "browser": false,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:node/recommended", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn"
    }
}
