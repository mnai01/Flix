        "framer-motion": "^5.6.0",
        "graphql": "^15.3.0",
        "next": "12.0.10",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.25.3"
    },
    "devDependencies": {
        "@types/graphql": "^14.5.0",
        "@types/node": "17.0.14",
        "@types/react": "17.0.38",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "apollo": "^2.33.9",
        "eslint": "8.8.0",
        "eslint-config-next": "12.0.10",
        "eslint-config-prettier": "^8.3.0",
