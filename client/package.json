{
    "name": "client",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "download-schema": "apollo service:download --endpoint=http://localhost:4000/graphql apollo/schema.json",
